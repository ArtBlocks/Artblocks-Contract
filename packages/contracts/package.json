{
  "name": "@artblocks/contracts",
  "version": "1.0.3",
  "description": "Smart contracts used by Art Blocks",
  "files": [
    "/contracts/**/*.sol",
    "/artifacts/contracts/**/!(*.dbg)*.json"
  ],
  "scripts": {
    "clean": "hardhat clean",
    "compile": "hardhat compile",
    "build": "hardhat --config build-package.config.ts compile",
    "docgen": "hardhat docgen",
    "generate:typechain": "typechain --target ethers-v5 --out-dir ./scripts/contracts './artifacts/contracts/**/!(*.dbg)*.json'",
    "test": "hardhat test",
    "size": "hardhat size-contracts",
    "test:parallel": "hardhat test --parallel",
    "coverage": "hardhat coverage",
    "format": "prettier --write 'contracts/**/*.sol' && prettier --write 'scripts/**/*.ts' && prettier --write 'test/**/*.ts'",
    "lint": "prettier --check 'contracts/**/*.sol' && prettier --check 'scripts/**/*.ts' && prettier --check 'test/**/*.ts' && npx eslint 'test/**/*.ts'",
    "remix": "remixd -s ./ --remix-ide https://remix.ethereum.org",
    "codegen": "yarn --cwd=\"../..\" codegen:project contracts",
    "deploy:dev:v3-engine": "yarn codegen && NODE_ENV=dev yarn hardhat run --network goerli scripts/engine/V3/generic-engine-deployer.ts",
    "deploy:dev-arbitrum:v3-engine": "yarn codegen && NODE_ENV=arbitrum-dev yarn hardhat run --network arbitrum-goerli scripts/engine/V3/generic-engine-deployer.ts",
    "deploy:staging-arbitrum:v3-engine": "yarn codegen && NODE_ENV=arbitrum-staging yarn hardhat run --network arbitrum-goerli scripts/engine/V3/generic-engine-deployer.ts",
    "deploy:arbitrum:v3-engine": "yarn codegen && NODE_ENV=arbitrum yarn hardhat run --network arbitrum scripts/engine/V3/generic-engine-deployer.ts",
    "deploy:staging:v3-engine": "yarn codegen && NODE_ENV=staging yarn hardhat run --network goerli scripts/engine/V3/generic-engine-deployer.ts",
    "deploy:mainnet:v3-engine": "yarn codegen && NODE_ENV=mainnet yarn hardhat run --network mainnet scripts/engine/V3/generic-engine-deployer.ts",
    "deploy:goerli:minter": "yarn hardhat run --network goerli scripts/minter-deployments/V3/generic-minter-deployer-v3core.ts",
    "deploy:mainnet:minter": "yarn hardhat run --network mainnet scripts/minter-deployments/V3/generic-minter-deployer-v3core.ts",
    "deploy:shared-randomizer:goerli": "yarn hardhat run --network goerli scripts/randomizer-deployments/shared-randomizer-deployer.ts",
    "deploy:shared-randomizer:mainnet": "yarn hardhat run --network mainnet scripts/randomizer-deployments/shared-randomizer-deployer.ts",
    "deploy:shared-randomizer:arbitrum-goerli": "yarn hardhat run --network arbitrum-goerli scripts/randomizer-deployments/shared-randomizer-deployer.ts",
    "deploy:shared-randomizer:arbitrum": "yarn hardhat run --network arbitrum scripts/randomizer-deployments/shared-randomizer-deployer.ts",
    "deploy:shared-minter-filter:goerli": "yarn hardhat run --network goerli scripts/minter-filter-deployments/shared-minter-filter-deployer.ts",
    "deploy:shared-minter-filter:mainnet": "yarn hardhat run --network mainnet scripts/minter-filter-deployments/shared-minter-filter-deployer.ts",
    "deploy:shared-minter-filter:arbitrum-goerli": "yarn hardhat run --network arbitrum-goerli scripts/minter-filter-deployments/shared-minter-filter-deployer.ts",
    "deploy:shared-minter-filter:arbitrum": "yarn hardhat run --network arbitrum scripts/minter-filter-deployments/shared-minter-filter-deployer.ts",
    "deploy:shared-shared-minters:goerli": "yarn hardhat run --network goerli scripts/minter-deployments/shared-minters-deployer.ts",
    "deploy:shared-shared-minters:mainnet": "yarn hardhat run --network mainnet scripts/minter-deployments/shared-minters-deployer.ts",
    "deploy:shared-shared-minters:arbitrum-goerli": "yarn hardhat run --network arbitrum-goerli scripts/minter-deployments/shared-minters-deployer.ts",
    "deploy:shared-shared-minters:arbitrum": "yarn hardhat run --network arbitrum scripts/minter-deployments/shared-minters-deployer.ts",
    "pre-commit": "yarn lint"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.386.0",
    "@aws-sdk/credential-providers": "^3.389.0",
    "@defi-wonderland/smock": "^2.3.5",
    "@gnosis.pm/safe-contracts": "^1.3.0",
    "@gnosis.pm/safe-core-sdk": "^2.1.0",
    "@gnosis.pm/safe-core-sdk-types": "^1.7.0",
    "@gnosis.pm/safe-ethers-lib": "1.1.0",
    "@gnosis.pm/safe-service-client": "^1.4.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.8",
    "@nomiclabs/hardhat-ethers": "^2.2.1",
    "@nomiclabs/hardhat-etherscan": "^3.1.4",
    "@nomiclabs/hardhat-solhint": "^3.0.0",
    "@nomiclabs/hardhat-truffle5": "^2.0.7",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@openzeppelin/hardhat-upgrades": "^1.28.0",
    "@openzeppelin/test-helpers": "^0.5.16",
    "@remix-project/remixd": "^0.6.16",
    "@typechain/ethers-v5": "^11.1.1",
    "@types/jest": "^29.5.3",
    "@types/mocha": "^10.0.1",
    "@types/prompt": "^1.1.5",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.3.0",
    "aws-sdk-client-mock": "^3.0.0",
    "chai": "^4.3.7",
    "coveralls": "^3.1.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.47.0",
    "eslint-plugin-mocha": "^10.1.0",
    "eslint-config-custom": "*",
    "ethereum-waffle": "^4.0.10",
    "ethers": "^5.7.2",
    "hardhat": "^2.17.1",
    "hardhat-contract-sizer": "^2.10.0",
    "hardhat-docgen": "^1.3.0",
    "hardhat-gas-reporter": "^1.0.9",
    "husky": "^8.0.3",
    "istanbul-combine": "^0.3.0",
    "keccak256": "^1.0.6",
    "merkletreejs": "^0.3.9",
    "prettier": "^3.0.1",
    "prettier-plugin-solidity": "^1.1.1",
    "prompt": "^1.3.0",
    "prompt-sync": "^4.2.0",
    "solidity-coverage": "^0.8.4",
    "ts-node": "^9.1.1",
    "tsconfig": "*",
    "typechain": "^8.3.1",
    "typescript": "^5.1.6"
  },
  "author": "Art Blocks, Inc",
  "license": "LGPL-3.0-only",
  "dependencies": {
    "@openzeppelin-0.5/contracts": "npm:@openzeppelin/contracts@2.5.1",
    "@openzeppelin-4.5/contracts": "npm:@openzeppelin/contracts@4.5.0",
    "@openzeppelin-4.7/contracts": "npm:@openzeppelin/contracts@4.7.1",
    "@openzeppelin-4.8/contracts-upgradeable": "npm:@openzeppelin/contracts-upgradeable@4.8.0",
    "@urql/exchange-retry": "^1.2.0",
    "graphql": "^16.7.1",
    "urql": "^4.0.5"
  }
}
